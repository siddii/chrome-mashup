{
  "name": "Mashup",
  "version": "0.1",
  "description": "Google Chrome extension for Web connoisseurs!",
    "background": {
        "persistent": false,
        "page": "background.html"
    },
    "permissions": [
    "bookmarks",
    "tabs"
  ],
  "browser_action": {
      "default_title": "Mashup",
      "default_icon": "icon.png"
  },
  "manifest_version": 2,
  "web_accessible_resources": [
        "angular/*.js",
        "jquery/*.js",
        "bootstrap/*.js"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}