{
    "name": "Mashup",
    "version": "1.0.0",
    "icons": {
        "128": "mashup-icon.jpg"
    },
    "description": "Chrome extension for web connoisseurs!",
    "permissions": [
        "bookmarks",
        "tabs",
        "https://ajax.googleapis.com/",
        "*://*/"
    ],
    "browser_action": {
        "default_title": "Chrome Mashup",
        "default_icon": "mashup-icon.jpg",
        "default_popup": "popup.html"
    },
    "manifest_version": 2,
    "web_accessible_resources": [
        "angular/*.js",
        "jquery/*.js",
        "bootstrap/*.js"
    ],
    "content_security_policy": "script-src https://ajax.googleapis.com/; script-src 'self' 'unsafe-eval'; object-src 'self'"
}